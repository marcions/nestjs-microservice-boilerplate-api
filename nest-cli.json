{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/common/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "copyFiles": true,
        "sync": true
      }
    },
    "webpack": true,
    "tsConfigPath": "apps/common/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/common",
  "projects": {
    "common": {
      "type": "application",
      "root": "apps/common",
      "entryFile": "main",
      "sourceRoot": "apps/common/src",
      "compilerOptions": {
        "tsConfigPath": "apps/common/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "cats": {
      "type": "application",
      "root": "apps/cats",
      "entryFile": "main",
      "sourceRoot": "apps/cats/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cats/tsconfig.app.json"
      }
    }
  }
}