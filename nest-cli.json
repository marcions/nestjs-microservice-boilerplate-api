{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "tsConfigPath": "apps/users/tsconfig.json",
    "webpack": true
  },
  "monorepo": true,
  "projects": {
    "@app/auth": {
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/auth",
      "sourceRoot": "apps/auth/src",
      "type": "application"
    },
    "@app/cats": {
      "compilerOptions": {
        "tsConfigPath": "apps/cats/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/cats",
      "sourceRoot": "apps/cats/src",
      "type": "application"
    },
    "@app/users": {
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/users",
      "sourceRoot": "apps/users/src",
      "type": "application"
    },
    "@libs/auth": {
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/auth",
      "sourceRoot": "libs/auth",
      "type": "library"
    },
    "@libs/core": {
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/core",
      "sourceRoot": "libs/core",
      "type": "library"
    },
    "@libs/infra": {
      "compilerOptions": {
        "tsConfigPath": "libs/infra/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/infra",
      "sourceRoot": "libs/infra",
      "type": "library"
    },
    "@libs/utils": {
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/utils",
      "sourceRoot": "libs/utils",
      "type": "library"
    },
    "@tools/eslint.config": {
      "entryFile": ".eslintrc.json",
      "root": "tools/eslint",
      "sourceRoot": "tools/eslint",
      "type": "library"
    }
  }
}
