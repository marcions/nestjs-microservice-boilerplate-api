{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "tsConfigPath": "apps/api/users/tsconfig.json",
    "webpack": true
  },
  "monorepo": true,
  "projects": {
    "@api/auth": {
      "compilerOptions": {
        "tsConfigPath": "apps/api/auth/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/api/auth",
      "sourceRoot": "apps/api/auth/src",
      "type": "application"
    },
    "@api/cats": {
      "compilerOptions": {
        "tsConfigPath": "apps/api/cats/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/api/cats",
      "sourceRoot": "apps/api/cats/src",
      "type": "application"
    },
    "@api/dogs": {
      "compilerOptions": {
        "tsConfigPath": "apps/api/dogs/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/api/dogs",
      "sourceRoot": "apps/api/dogs/src",
      "type": "application"
    },
    "@api/users": {
      "compilerOptions": {
        "tsConfigPath": "apps/api/users/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/api/users",
      "sourceRoot": "apps/api/users/src",
      "type": "application"
    },
    "@core": {
      "compilerOptions": {
        "tsConfigPath": "core/tsconfig.json"
      },
      "entryFile": "index",
      "root": "core",
      "sourceRoot": "core",
      "type": "library"
    },
    "@libs/auth": {
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/auth",
      "sourceRoot": "libs/auth",
      "type": "library"
    },
    "@libs/infra": {
      "compilerOptions": {
        "tsConfigPath": "libs/infra/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/infra",
      "sourceRoot": "libs/infra",
      "type": "library"
    },
    "@libs/utils": {
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.json"
      },
      "entryFile": "index",
      "root": "libs/utils",
      "sourceRoot": "libs/utils",
      "type": "library"
    },
    "@tools/eslint.config": {
      "entryFile": ".eslintrc.json",
      "root": "tools/eslint",
      "sourceRoot": "tools/eslint",
      "type": "library"
    },
    "@workers/dogs": {
      "compilerOptions": {
        "tsConfigPath": "apps/workers/dogs/tsconfig.json"
      },
      "entryFile": "main",
      "root": "apps/workers/dogs",
      "sourceRoot": "apps/workers/dogs/src",
      "type": "application"
    }
  }
}