{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/common/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "copyFiles": true,
        "sync": true
      }
    },
    "webpack": true,
    "tsConfigPath": "apps/common/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/common",
  "projects": {
    "common": {
      "type": "application",
      "root": "apps/common",
      "entryFile": "main",
      "sourceRoot": "apps/common/src",
      "compilerOptions": {
        "tsConfigPath": "apps/common/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "cat": {
      "type": "application",
      "root": "apps/cat",
      "entryFile": "main",
      "sourceRoot": "apps/cat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cat/tsconfig.app.json"
      }
    },
    "dog": {
      "type": "application",
      "root": "apps/dog",
      "entryFile": "main",
      "sourceRoot": "apps/dog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dog/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}